<h2>Add a New Feed</h2>

<% form_for Feed.new, :url => admin_feeds_path do |f| %>
	
	<%= error_messages_for :feed %>
	
	<fieldset>
		
		<%= render :partial => "admin/feeds/form", :locals => { :f => f } %>

		<div class="submit">
			<p><%= f.submit 'Add Feed' %> or <%= link_to 'Cancel', admin_root_path %></p>
		</div>
		
	</fieldset>

<% end %>