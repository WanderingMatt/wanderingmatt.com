<% unless @items.empty? %>
	
	<ol id="lifestream">
		<% @items.each_with_index do |item, index| %>
			<%= render :partial => 'items/item', :locals => { :item => item, :offset => @offset, :item_unique_id => @total, :index => index } %>
			<% @total-=(1+item.grouped_items.length) %>
		<% end %>
	</ol>
	
<% end %>