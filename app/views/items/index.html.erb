<pre>

<% @items.to_yaml %>

<% @items.each_with_index do |item, key| -%>

<%= key %>. <%= item.feed.permalink %> &ndash; <%= item.title %>
<% unless item.grouped_items.empty? -%>
<% for i in item.grouped_items -%>
	&bull; <%= i.title %>
<% end -%>
<% end %>
<% end -%>

</pre>


